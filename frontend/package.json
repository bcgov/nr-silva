{
  "name": "nr-silva-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@bcgov-nr/nr-theme": "^1.7.0",
    "@carbon/charts": "^1.22.7",
    "@carbon/charts-react": "^1.22.5",
    "@carbon/icons-react": "^11.53.0",
    "@carbon/pictograms-react": "^11.69.0",
    "@carbon/react": "^1.77.0",
    "@tanstack/react-query": "^5.50.1",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@types/carbon__pictograms-react": "^11.46.10",
    "@types/node": "^24.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "aws-amplify": "^6.12.2",
    "axios": "^1.6.8",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "ol": "^10.6.1",
    "proj4": "^2.19.10",
    "qs": "^6.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.0.0",
    "tokml": "^0.4.0",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^5.0.0"
  },
  "scripts": {
    "dev": "start-server-and-test stub http://127.0.0.1:8080 start",
    "start": "vite --host",
    "prebuild": "tsc",
    "build": "vite build",
    "build:env": "echo \"window.config = { \\\"VITE_USER_POOLS_ID\\\": \\\"ca-central-1_UpeAqsYt4\\\", \\\"VITE_USER_POOLS_WEB_CLIENT_ID\\\": \\\"4e7gnonatuf3q7pfcpv7d8gfiv\\\", \\\"VITE_ZONE\\\": \\\"TEST\\\" };\" > build/env.js",
    "preview": "vite preview --port 4173",
    "generate:openapi": "rm -rf ./src/services/OpenApi && npx bcgov-typescript-codegen --input http://localhost:8080/v3/api-docs --output ./src/services/OpenApi --client axios",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "precoverage": "rm -rf .nyc_output coverage",
    "coverage": " npm run test:unit && npm run test:ui",
    "postcoverage": "mkdir -p coverage && touch coverage/coverage-final.json && cp coverage/coverage-final.json .nyc_output/unit-test.json && nyc merge .nyc_output .nyc_output/coverage.json && nyc report",
    "test:unit": "vitest run --coverage --watch=false --update",
    "test:unit:dev": "vitest --reporter=verbose",
    "test:ui": "playwright test",
    "test:ui:dev": "playwright test --ui",
    "stub": "wiremock --enable-stub-cors --global-response-templating --port 8080 --https-port 8081 --root-dir ./stub --verbose"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.54.0",
    "@testing-library/dom": "^10.2.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^24.0.0",
    "@types/qs": "^6.9.16",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.4",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/coverage-v8": "^3.0.5",
    "bcgov-typescript-codegen": "^1.0.9",
    "dotenv": "^17.0.0",
    "eslint": "^8.48.0",
    "eslint-config-love": "^43.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^48.2.7",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^27.0.0",
    "nyc": "^17.1.0",
    "sass": "^1.62.1",
    "sass-loader": "^16.0.0",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.2.2",
    "v8-to-istanbul": "^9.3.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.5",
    "wiremock": "^3.13.1"
  },
  "overrides": {
    "braces@3.0.2": "3.0.3",
    "fast-xml-parser@4.4.0": "4.4.1",
    "ws@8.17.0": "8.17.1",
    "@babel/runtime": "7.28.4",
    "@babel/helpers": "7.28.4",
    "react-router": "7.9.6",
    "minimist": "1.2.8",
    "uglify-js": "3.19.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "reporter": [
      "html",
      "lcov"
    ],
    "report-dir": ".nyc_output/coverage",
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.spec.ts",
      "**/__e2e__/**",
      "**/__test__/**",
      "**/assets/**",
      "**/Landing/index.tsx",
      "**/constants.ts",
      "**/constants.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": false
  }
}
